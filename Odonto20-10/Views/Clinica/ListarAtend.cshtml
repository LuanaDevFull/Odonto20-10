@model IEnumerable<Odonto20_10.Models.ModelAtendimento>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.codAtendimento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.dataAtendimento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.horaDentista)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nmPaciente)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nmDentista)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.codAtendimento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.dataAtendimento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.horaDentista)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nmPaciente)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nmDentista)
        </td>
        <td>
            @Html.ActionLink("Atualizar", "editarAtend", new { id = item.codAtendimento }) |
            @Html.ActionLink("Excluir", "excluirAtend", new { id = item.codAtendimento }, new { onclick = "return confirm('Deseja realmente excluir?');" })
        </td>
    </tr>
}

</table>
